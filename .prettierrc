{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "endOfLine": "auto",
  "printWidth": 180,
  "htmlWhitespaceSensitivity": "ignore",
  "arrowParens": "always",
  "plugins": ["prettier-plugin-tailwindcss"]
}
